{
  "$schema": "https://schema.tauri.app/config/2",
  "productName": "pashe-frontend",
  "version": "0.1.0",
  "identifier": "cc.chaosindex",
  "build": {
    "beforeDevCommand": "bun run dev --host",
    "beforeBuildCommand": "",
    "devUrl": "http://localhost:1420",
    "frontendDist": "http://localhost:1420"
  },
  "app": {
    "windows": [
      {
        "title": "pashe-frontend",
        "width": 800,
        "height": 600,
        "url": "http://localhost:1420"
      }
    ],
    "security": {
      "csp": null
    }
  },
  "bundle": {
    "active": true,
    "targets": "all",
    "icon": [
      "icons/32x32.png",
      "icons/128x128.png",
      "icons/128x128@2x.png",
      "icons/icon.icns",
      "icons/icon.ico"
    ]
  }
}
